{
    "extends": "airbnb-base/legacy",
    "rules": {
        "no-param-reassign": 0,
        "func-names": 0,
        "no-console": 0,
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "vars-on-top": 0,
        "no-plusplus": 0,
        "max-len": 0,
        "no-restricted-syntax": 0,
        "no-unused-vars": [2, { "varsIgnorePattern": "(BKGWebMap|okapi)" }],
        "no-underscore-dangle": 0,
        "guard-for-in": 0,
        "consistent-return": 0,
        "no-continue": 0,
        "no-loop-func": 0,
        "no-use-before-define": 0,
        "no-restricted-globals": 0,
        "no-shadow": 0,
        "no-mixed-operators": 0,
        "no-alert": 0,
        "no-script-url": 0,
        "new-cap": 0,
        "no-inner-declarations": 0,
        "no-extend-native": 0,
        "no-template-curly-in-string": 0,
        "no-useless-return": 0,
        "no-else-return": 0
    },
    "globals": {
        "XLSX": false,
        "saveAs": false,
        "ol": false,
        "proj4": false,
        "BKGWebMap": false,
        "Uint8Array": false
    },
    "env": {
        "browser": true,
        "jasmine": true,
        "protractor": true
    }
}